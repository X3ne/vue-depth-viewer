{
  "name": "vue-depth-viewer",
  "version": "1.1.0",
  "description": "Vue component to create a 3d effect on images with a depthmap using WebGL",
  "author": "X3ne",
  "homepage": "https://github.com/X3ne/vue-depth-viewer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/X3ne/vue-depth-viewer"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "npm",
    "depthmap",
    "depth",
    "image",
    "ts"
  ],
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/index.min.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext .ts --ext .tsx"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^4.9.5",
    "vite": "^5.0.7",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-glsl": "^1.2.1",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "vue-demi": "^0.14.6"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
